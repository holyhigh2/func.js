{
    "disableSources":true,
    "visibilityFilters": {
        "protected": false,
        "private": false,
        "inherited": true,
        "external": false,
    },
    "excludeInternal": true,
    "hideBreadcrumbs":true,
    "hideInPageTOC":true, 
    "entryPoints": [
        // "src/index.ts",
        "src/array.ts",
        "src/chaining.ts",
        "src/collection.ts",
        "src/datetime.ts",
        "src/functions.ts",
        "src/is.ts",
        "src/math.ts",
        "src/number.ts",
        "src/object.ts",
        "src/string.ts",
        "src/template.ts",
        "src/tree.ts",
        "src/utils.ts",
        "src/types.ts",
    ],
    "out": "docs",
    "plugin": [
        "typedoc-plugin-markdown"
    ],
    "theme": "markdown",
    "tsconfig": "./tsconfig.json"
}